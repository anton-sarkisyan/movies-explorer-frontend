{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-nested-ternary": "off",
      "no-unused-expressions": "off",
      "react/prop-types": "off",
      "no-shadow": "off",
      "no-underscore-dangle": ["error", {"allow": ["_id"]}]
    }
}

